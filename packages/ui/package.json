{
  "name": "@krds-prac/ui",
  "version": "0.0.10",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/esm/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 --fix",
    "prepare": "panda codegen",
    "generate:component": "turbo gen react-component",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "codegen": "pnpm panda codegen",
    "cssgen": "pnpm panda cssgen",
    "panda:dev": "pnpm panda --watch",
    "studio": "pnpm panda studio",
    "pub": "pnpm codegen && pnpm cssgen && pnpm publish --no-git-checks",
    "build:comp": "rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@krds-prac/eslint-config": "workspace:*",
    "@krds-prac/prettier-config": "workspace:*",
    "@krds-prac/typescript-config": "workspace:*",
    "@pandacss/dev": "^0.45.2",
    "@pandacss/studio": "^0.46.1",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-controls": "^8.2.9",
    "@storybook/addon-docs": "^8.2.9",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/preview-api": "^8.2.9",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@storybook/test-runner": "^0.19.1",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "concurrently": "^8.2.2",
    "eslint-plugin-storybook": "^0.10.1",
    "prop-types": "^15.8.1",
    "storybook": "^8.3.6",
    "tsc-alias": "^1.8.10",
    "typescript-transform-paths": "^3.5.1"
  },
  "dependencies": {
    "@krds-prac/preset": "workspace:*",
    "@pandacss/dev": "^0.45.2",
    "@vitejs/plugin-react": "^4.3.3",
    "react": "^18.2.0",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
