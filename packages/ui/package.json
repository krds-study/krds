{
  "name": "@krds-prac/ui",
  "version": "0.0.10",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/esm/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 --fix",
    "generate:component": "turbo gen react-component",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "codegen": "pnpm panda codegen",
    "cssgen": "pnpm panda cssgen -o dist/styles.css",
    "panda:dev": "pnpm panda --watch",
    "studio": "pnpm panda studio",
    "pub": "pnpm codegen && pnpm cssgen && pnpm publish --no-git-checks",
    "build:comp": "rollup -c"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@krds-prac/eslint-config": "workspace:*",
    "@krds-prac/prettier-config": "workspace:*",
    "@krds-prac/typescript-config": "workspace:*",
    "@pandacss/dev": "^0.45.2",
    "@pandacss/studio": "^0.46.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "concurrently": "^8.2.2",
    "rollup": "^4.21.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tsc-alias": "^1.8.10",
    "typescript-transform-paths": "^3.5.1"
  },
  "dependencies": {
    "@krds-prac/preset": "workspace:*",
    "@pandacss/dev": "^0.45.2",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
